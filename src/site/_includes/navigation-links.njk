  <ul class="inline-block">
  {% for link in linkList %}
    <li class="block py-1 mr-6 md:inline md:mr-10">
      <a href="{{ link.url }}"
        class="font-semibold {% if page.url.split("/")[1] == link.url.split("/")[1] %} text-red-500 {% endif %}">
        {{ link.text }}
      </a>
    </li>
  {% endfor %}
  </ul>
